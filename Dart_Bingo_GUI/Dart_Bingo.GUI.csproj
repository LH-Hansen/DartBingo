<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Resources\Dart.ico" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Dart_Bingo\Dart_Bingo.Service.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Resources\Dart.ico">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </Resource>
  </ItemGroup>

</Project>
